# Docker settings

SOKETI_DEBUG=0
SOKETI_METRICS_ENABLED=false
METRICS_SERVER_PORT=9601
DEFAULT_APP_ENABLE_CLIENT_MESSAGES=true
DEFAULT_APP_ID="${PUSHER_APP_ID}"
