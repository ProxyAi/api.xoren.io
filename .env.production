# Deployment

DEPLOYMENT_VERSION="<DEPLOYMENT_VERSION>"
DEPLOYMENT_ENV=production
DEPLOYMENT_TIMEZONE=Europe/London
DEPLOYMENT_ENCODE=en_GB
DEPLOYMENT_AT="<DEPLOYMENT_AT>"

# Laravel settings

APP_NAME=XOREN.IO
APP_ENV=production
APP_KEY="<APP_KEY>"
APP_DEBUG=false
APP_URL=https://api.xoren.io


# Docker settings

APP_USER_UUID="<APP_USER_UUID>"
APP_USER="<APP_USER>"
APP_USER_GUID="<APP_USER_GUID>"

TZ=Europe/London
APP_TIMEZONE="${TZ}"
APP_ENCODE=en_GB


# Laravel settings

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug


# Laravel & Docker settings

DB_CONNECTION=mysql
DB_HOST="<DB_HOST>"
DB_PORT=3306
DB_DATABASE="<DB_DATABASE>"
DB_USERNAME="<DB_USERNAME>"
DB_PASSWORD="<DB_PASSWORD>"

## Mysql settings
FORWARD_DB_PORT="<FORWARD_DB_PORT>"
MYSQL_ADMIN_USER="root"
MYSQL_ROOT_PASSWORD="password"
MYSQL_ALLOW_EMPTY_PASSWORD=0
INIT_ROCKSDB=1


# Session settings

BROADCAST_DRIVER=pusher
CACHE_DRIVER=redis
FILESYSTEM_DISK=local
QUEUE_CONNECTION=redis
#SESSION_DOMAIN=".${APP_HOSTNAME}"
SESSION_DRIVER=cookie
SESSION_LIFETIME=2440
SANCTUM_STATEFUL_DOMAINS=.${APP_HOSTNAME}:443,.pages.dev:443

REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379


# Mailer settings

MAIL_MAILER="<MAIL_MAILER>"
MAIL_HOST="<MAIL_HOST>"
